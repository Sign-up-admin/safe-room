{
    "java.compile.nullAnalysis.mode":  "automatic",
    "java.configuration.updateBuildConfiguration":  "interactive",
    "git.path":  "C:\\Users\\李\\AppData\\Local\\GitHubDesktop\\app-3.5.4\\resources\\app\\git\\cmd\\git.exe",
    
    // ========== 文件系统监视配置（修复冲突问题）==========
    
    // 文件系统监视 - 启用以检测磁盘文件变化，避免冲突提示
    "files.watchFiles": "default",
    "files.useExperimentalFileWatcher": false,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/build/**": true,
        "**/.vscode/**": true,
        "**/.idea/**": true,
        "**/target/**": true,
        "**/out/**": true,
        "**/coverage/**": true,
        "**/.next/**": true,
        "**/.nuxt/**": true,
        "**/.cache/**": true,
        "**/tmp/**": true,
        "**/temp/**": true,
        "**/*.log": true,
        "**/*.tmp": true
    },
    
    // 自动保存 - 启用以确保文件写入磁盘（关键修复）
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.hotExit": "onExit",
    
    // 文件冲突解决 - 自动覆盖磁盘文件（编辑器版本优先）
    // 这样可以避免大量冲突提示，同时确保编辑器的修改被保存
    "files.saveConflictResolution": "overwriteFileOnDisk",
    // 文件参与者超时（已在下面设置）
    // "files.participants.timeout": 5000,
    "files.trimTrailingWhitespace": false,
    "files.insertFinalNewline": false,
    "files.trimFinalNewlines": false,
    "files.eol": "\n",
    
    // 文件锁定和权限
    "files.readonlyInclude": {},
    "files.readonlyExclude": {},
    "files.readonlyFromPermissions": false,
    
    // 自动刷新 - 启用以同步磁盘文件状态
    "files.refreshOnWindowFocus": true,
    "files.refreshOnActivate": true,
    "explorer.autoReveal": false,
    "explorer.fileNesting.enabled": false,
    
    // Git自动操作 - 完全禁用
    "git.autoRefresh": false,
    "git.enableSmartCommit": false,
    "git.autofetch": false,
    "git.autofetchPeriod": 0,
    "git.confirmSync": true,
    "git.enableStatusBarSync": false,
    "git.suggestSmartCommit": false,
    "git.timeline.enabled": false,
    "git.decorations.enabled": false,
    
    // ESLint - 禁用所有自动功能
    "eslint.enable": false,
    "eslint.format.enable": false,
    "eslint.lintTask.enable": false,
    "eslint.run": "never",
    "eslint.validate": [],
    "eslint.workingDirectories": [],
    
    // 编辑器自动操作 - 完全禁用
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.codeActionsOnSave": {},
    "editor.suggestOnTriggerCharacters": false,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.parameterHints.enabled": false,
    "editor.hover.enabled": false,
    "editor.lightbulb.enabled": "off",
    "editor.codeLens": false,
    "editor.inlineSuggest.enabled": false,
    "editor.suggestSelection": "first",
    "editor.wordBasedSuggestions": "off",
    "editor.snippetSuggestions": "none",
    
    // TypeScript自动检查 - 禁用
    "typescript.updateImportsOnFileMove.enabled": "never",
    "typescript.suggest.autoImports": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.tsserver.watchOptions": {
        "watchFile": "none",
        "watchDirectory": "none",
        "fallbackPolling": "disabled"
    },
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.surveys.enabled": false,
    "typescript.tsserver.experimental.enableProjectDiagnostics": false,
    
    // JavaScript自动检查 - 禁用
    "javascript.updateImportsOnFileMove.enabled": "never",
    "javascript.suggest.autoImports": false,
    "javascript.preferences.importModuleSpecifier": "relative",
    
    // Vue自动功能 - 禁用
    "vetur.format.enable": false,
    "vetur.validation.template": false,
    "vetur.validation.script": false,
    "vetur.validation.style": false,
    "volar.autoCompleteRefs": false,
    "volar.codeLens.pugTools": false,
    "volar.codeLens.scriptSetupTools": false,
    
    // 搜索和索引 - 禁用自动更新
    "search.useIgnoreFiles": true,
    "search.useGlobalIgnoreFiles": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/build": true,
        "**/.git": true,
        "**/.vscode": true
    },
    "search.followSymlinks": false,
    "search.smartCase": false,
    "files.exclude": {},
    
    // 扩展自动功能 - 禁用
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": false,
    "extensions.ignoreRecommendations": true,
    
    // 终端自动操作 - 禁用
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    
    // 其他自动功能 - 禁用
    "update.mode": "manual",
    "update.showReleaseNotes": false,
    "telemetry.telemetryLevel": "off",
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.restoreViewState": false,
    
    // 调试自动功能 - 禁用
    "debug.allowBreakpointsEverywhere": false,
    "debug.inlineValues": false,
    "debug.showBreakpointsInOverviewRuler": false,
    
    // 任务自动运行 - 禁用
    "task.autoDetect": "off",
    "task.quickOpen.history": 0,
    "task.quickOpen.detail": false,
    
    // 问题面板自动更新 - 禁用
    "problems.autoReveal": false,
    "problems.showCurrentInStatus": false,
    
    // ========== 防止文件冲突的额外设置 ==========
    
    // 启用文件备份以确保数据安全（但不会阻止写入）
    "files.backup": true,
    "files.restoreUndoStack": true,
    
    // 启用文件系统事件监听以确保文件写入
    "files.useTrash": true,
    "files.enableTrash": true,
    
    // 禁用文件关联检查
    "files.associations": {},
    "files.defaultLanguage": "",
    
    // 禁用文件编码自动检测
    "files.autoGuessEncoding": false,
    "files.encoding": "utf8",
    
    // 文件大小限制（已在下面设置）
    // "files.maxMemoryForLargeFilesMB": 4096,
    
    // 禁用编辑器自动重新加载
    "editor.detectIndentation": false,
    "editor.autoIndent": "none",
    "editor.formatOnSaveMode": "off",
    
    // 文件内容提供者设置 - 优化文件操作行为
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.closeOnFileMove": true,
    "workbench.editor.revealIfOpen": true,
    
    // 文件系统提供者设置（已在下面设置）
    // "files.simpleDialog.enable": true,
    "files.dialog.defaultPath": "",
    
    // 文件时间戳检查 - 允许修改时间戳以确保文件真正写入
    "files.preserveFileTimestamps": false,
    
    // ========== 强制文件写入磁盘的关键设置 ==========
    
    // 确保文件立即写入磁盘，不使用缓存
    // 注意：files.useTrash 和 files.enableTrash 已在上面设置
    
    // 禁用可能导致缓存问题的预览模式（已在上面设置，此处仅作说明）
    // workbench.editor.enablePreview 和 workbench.editor.enablePreviewFromQuickOpen 已在上面设置为 false
    
    // 确保保存时立即写入（已在上面设置）
    // files.saveConflictResolution 已在上面设置为 "overwriteFileOnDisk"（自动覆盖，避免冲突提示）
    
    // 文件系统提供者设置 - 确保直接写入（已在上面设置）
    // files.simpleDialog.enable 已在上面设置为 true
    
    // 文件参与者超时设置（已在上面设置为 5000ms）
    
    // ========== 额外的文件写入优化设置 ==========
    
    // 确保文件系统操作立即执行，不使用延迟写入
    "files.maxMemoryForLargeFilesMB": 4096,
    
    // 禁用可能导致文件写入延迟的优化
    "files.enableWatcherPolling": false,
    
    // 确保文件保存时立即刷新到磁盘
    "files.saveDelay": 0,
    
    // 工作区信任设置 - 确保文件操作权限
    "security.workspace.trust.enabled": true,
    "security.workspace.trust.untrustedFiles": "prompt",
    
    // 文件系统提供者 - 使用原生文件系统以确保可靠性
    "files.simpleDialog.enable": true,
    
    // 禁用可能导致文件写入问题的实验性功能
    "files.experimental.trimOnSave": false,
    
    // 确保文件保存时不会因为其他操作而延迟
    "files.participants.timeout": 5000
}
