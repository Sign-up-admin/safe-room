{
    "java.compile.nullAnalysis.mode":  "automatic",
    "java.configuration.updateBuildConfiguration":  "interactive",
    "git.path":  "C:\\Users\\李\\AppData\\Local\\GitHubDesktop\\app-3.5.4\\resources\\app\\git\\cmd\\git.exe",
    
    // ========== 全面禁用自动跟踪和监视 ==========
    
    // 文件系统监视 - 完全关闭
    "files.watchFiles": "off",
    "files.useExperimentalFileWatcher": false,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/build/**": true,
        "**/.vscode/**": true,
        "**/.idea/**": true,
        "**/target/**": true,
        "**/out/**": true,
        "**/coverage/**": true,
        "**/.next/**": true,
        "**/.nuxt/**": true,
        "**/.cache/**": true,
        "**/tmp/**": true,
        "**/temp/**": true,
        "**/*.log": true,
        "**/*.tmp": true
    },
    
    // 自动保存 - 完全禁用
    "files.autoSave": "off",
    "files.autoSaveDelay": 0,
    "files.hotExit": "off",
    
    // 文件冲突解决 - 强制覆盖磁盘文件，避免冲突提示
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "files.participants.timeout": 0,
    "files.trimTrailingWhitespace": false,
    "files.insertFinalNewline": false,
    "files.trimFinalNewlines": false,
    "files.eol": "\n",
    
    // 文件锁定和权限
    "files.readonlyInclude": {},
    "files.readonlyExclude": {},
    "files.readonlyFromPermissions": false,
    
    // 自动刷新 - 完全禁用
    "files.refreshOnWindowFocus": false,
    "files.refreshOnActivate": false,
    "explorer.autoReveal": false,
    "explorer.fileNesting.enabled": false,
    
    // Git自动操作 - 完全禁用
    "git.autoRefresh": false,
    "git.enableSmartCommit": false,
    "git.autofetch": false,
    "git.autofetchPeriod": 0,
    "git.confirmSync": true,
    "git.enableStatusBarSync": false,
    "git.suggestSmartCommit": false,
    "git.timeline.enabled": false,
    "git.decorations.enabled": false,
    
    // ESLint - 禁用所有自动功能
    "eslint.enable": false,
    "eslint.format.enable": false,
    "eslint.lintTask.enable": false,
    "eslint.run": "never",
    "eslint.validate": [],
    "eslint.workingDirectories": [],
    
    // 编辑器自动操作 - 完全禁用
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.codeActionsOnSave": {},
    "editor.suggestOnTriggerCharacters": false,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.parameterHints.enabled": false,
    "editor.hover.enabled": false,
    "editor.lightbulb.enabled": "off",
    "editor.codeLens": false,
    "editor.inlineSuggest.enabled": false,
    "editor.suggestSelection": "first",
    "editor.wordBasedSuggestions": "off",
    "editor.snippetSuggestions": "none",
    
    // TypeScript自动检查 - 禁用
    "typescript.updateImportsOnFileMove.enabled": "never",
    "typescript.suggest.autoImports": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.tsserver.watchOptions": {
        "watchFile": "none",
        "watchDirectory": "none",
        "fallbackPolling": "disabled"
    },
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.surveys.enabled": false,
    "typescript.tsserver.experimental.enableProjectDiagnostics": false,
    
    // JavaScript自动检查 - 禁用
    "javascript.updateImportsOnFileMove.enabled": "never",
    "javascript.suggest.autoImports": false,
    "javascript.preferences.importModuleSpecifier": "relative",
    
    // Vue自动功能 - 禁用
    "vetur.format.enable": false,
    "vetur.validation.template": false,
    "vetur.validation.script": false,
    "vetur.validation.style": false,
    "volar.autoCompleteRefs": false,
    "volar.codeLens.pugTools": false,
    "volar.codeLens.scriptSetupTools": false,
    
    // 搜索和索引 - 禁用自动更新
    "search.useIgnoreFiles": true,
    "search.useGlobalIgnoreFiles": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/build": true,
        "**/.git": true,
        "**/.vscode": true
    },
    "search.followSymlinks": false,
    "search.smartCase": false,
    "files.exclude": {},
    
    // 扩展自动功能 - 禁用
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": false,
    "extensions.ignoreRecommendations": true,
    
    // 终端自动操作 - 禁用
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    
    // 其他自动功能 - 禁用
    "update.mode": "manual",
    "update.showReleaseNotes": false,
    "telemetry.telemetryLevel": "off",
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.restoreViewState": false,
    
    // 调试自动功能 - 禁用
    "debug.allowBreakpointsEverywhere": false,
    "debug.inlineValues": false,
    "debug.showBreakpointsInOverviewRuler": false,
    
    // 任务自动运行 - 禁用
    "task.autoDetect": "off",
    "task.quickOpen.history": 0,
    "task.quickOpen.detail": false,
    
    // 问题面板自动更新 - 禁用
    "problems.autoReveal": false,
    "problems.showCurrentInStatus": false,
    
    // ========== 防止文件冲突的额外设置 ==========
    
    // 禁用文件备份和恢复
    "files.backup": false,
    "files.restoreUndoStack": false,
    
    // 禁用文件系统事件监听
    "files.useTrash": false,
    "files.enableTrash": false,
    
    // 禁用文件关联检查
    "files.associations": {},
    "files.defaultLanguage": "",
    
    // 禁用文件编码自动检测
    "files.autoGuessEncoding": false,
    "files.encoding": "utf8",
    
    // 禁用文件大小限制检查
    "files.maxMemoryForLargeFilesMB": 4096,
    
    // 禁用编辑器自动重新加载
    "editor.detectIndentation": false,
    "editor.autoIndent": "none",
    "editor.formatOnSaveMode": "off",
    
    // 禁用文件内容提供者
    "workbench.editor.closeOnFileDelete": false,
    "workbench.editor.closeOnFileMove": false,
    "workbench.editor.revealIfOpen": false,
    
    // 禁用文件系统提供者
    "files.simpleDialog.enable": true,
    "files.dialog.defaultPath": "",
    
    // 禁用文件时间戳检查
    "files.preserveFileTimestamps": false
}
