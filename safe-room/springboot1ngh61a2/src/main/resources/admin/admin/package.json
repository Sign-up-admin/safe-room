{
  "name": "gym-admin-frontend",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:check": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "check": "npm run lint:check && npm run format:check && npm run type-check",
    "check:fix": "npm run lint && npm run format",
    "test:unit": "vitest run --config ./vitest.config.ts",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:fast": "vitest run --config ./vitest.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "test:coverage:report": "vitest run --coverage && echo Coverage report generated in ./coverage/index.html",
    "test:coverage:ci": "vitest run --coverage --reporter=verbose --reporter=json --outputFile=coverage/coverage-summary.json",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:p2p": "playwright test tests/e2e/p2p-integration.spec.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "element-plus": "^2.8.8",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^20.0.10",
    "prettier": "^3.4.2",
    "sass": "^1.69.5",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.8",
    "vitest": "^4.0.9",
    "vue-tsc": "^1.8.25"
  }
}
